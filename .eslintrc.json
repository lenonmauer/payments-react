{
    "env": {
        "browser": true,
        "es6": true,
        "jest": true
    },
    "parser": "babel-eslint",
    "plugins": [
      "react"
    ],
    "extends": [
        "airbnb"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "rules": {
      "react/jsx-filename-extension": "off",
      "brace-style": ["error", "stroustrup"],
      "import/no-cycle": "off",
      "import/prefer-default-export": "off",
      "no-template-curly-in-string": "off"
    },
    "settings": {
      "import/resolver": {
        "alias": {
          "map": [["~", "./src"]]
        }
      }
    }
}
